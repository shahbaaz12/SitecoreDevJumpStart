@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>

    
   
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/clean-blog.min.css" rel="stylesheet" />
    <link href="~/Plugins/OwlCarousel/owl.carousel.min.css" rel="stylesheet"/>
    <link href="~/Plugins/OwlCarousel/owl.theme.default.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Content/clean-blog.min.js"></script>
    <script src="~/Plugins/OwlCarousel/owl.carousel.min.js"></script>

    <title>@Html.Sitecore().Field("title", new { DisableWebEdit = true })</title>
    @*@Html.Sitecore().VisitorIdentification()*@
</head>
<body>
    <h1>Layout</h1>
    <h1>@Html.Sitecore().Field("title")</h1>
    <div>
        <div class="container">
            <div class="row">
                @Html.Sitecore().Placeholder("main")
            </div>
        </div>

    </div>
   <script>
       $(document).ready(function () {
           $(".owl-carousel").owlCarousel();
       });
   </script>
</body>
</html>